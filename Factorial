def factorial(num):
    fact=1
    while(num>0):
        fact=fact*num
        num=num-1
    return(fact)
n=int(input("Enter Value of n : "))
sum=0
for a in range(1,n+1):
    sum=sum+factorial(a)
print("Sum of Factorials : ",sum)
